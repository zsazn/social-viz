{
  "name": "social-viz",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "author": "Zheng Zhou",
  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc --properties main es2015 browser module",
    "start": "ng serve",
    "build:dev": "ng build --prod --sourceMap",
    "build:prod": "ng build --prod",
    "lint": "ng lint && npm run stylelint",
    "stylelint": "stylelint **/*.scss",
    "test": "ng test --code-coverage=true",
    "testing-start": "echo ---------------Running Test cases----------------",
    "testing": "ng test --no-watch --browsers ChromeHeadless",
    "testing-end": "echo ---------------All Test cases passed----------------",
    "linter-start": "echo ---------------Running Linter----------------",
    "linter": "ng lint",
    "linter-end": "echo ---------------Linter Over: No Errors Found----------------",
    "e2e": "ng e2e",
    "pre-commit": "npm run linter-start && npm run linter && npm run linter-end && npm run testing-start && npm run testing && npm run testing-end"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit",
      "pre-push": "npm run build:dev"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.12",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.12",
    "@angular/compiler": "~9.1.12",
    "@angular/core": "~9.1.12",
    "@angular/forms": "~9.1.12",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.12",
    "@angular/platform-browser-dynamic": "~9.1.12",
    "@angular/router": "~9.1.12",
    "d3": "^5.16.0",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular/cli": "~9.1.12",
    "@angular/compiler-cli": "~9.1.12",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "husky": "^4.3.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "node-sass": "^4.14.1",
    "protractor": "~7.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "selector-type-no-unknown": [
        true,
        {
          "ignoreTypes": [
            "/^mat-/",
            "/^app-/"
          ]
        }
      ],
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "include",
            "function",
            "if",
            "else",
            "return",
            "warn",
            "mixin",
            "content"
          ]
        }
      ]
    }
  }
}
